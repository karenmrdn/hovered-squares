{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "quotes": ["error", "double"],
    "jsx-quotes": ["error", "prefer-double"],
    "max-len": ["warn", { "code": 150 }],
    "comma-dangle": ["error", "only-multiline"],
    "semi": ["error", "always"],
    "indent": "off"
  }
}
